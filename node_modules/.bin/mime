#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/layout-express/node_modules/mime/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/layout-express/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/node_modules:/mnt/c/Users/lelne/OneDrive/node_modules:/mnt/c/Users/lelne/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/layout-express/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/layout-express/node_modules/mime/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/layout-express/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/node_modules:/mnt/c/Users/lelne/OneDrive/node_modules:/mnt/c/Users/lelne/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/lelne/OneDrive/Documentos/SENA/ExpressJS/tienda-server/VOCERO/layout-express/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
